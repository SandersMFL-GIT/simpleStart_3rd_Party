    <formulas>
        <name>Recipients_Att_Para</name>
        <dataType>String</dataType>
        <expression>IF(
  ISBLANK({!varRespAttyEmail}),
  &apos;&apos;,                       /* nothing */
  {!varRespAttyEmail}
) &amp;
IF(
  AND(
      NOT(ISBLANK({!varRespAttyEmail})),
      NOT(ISBLANK({!varRespParaEmail}))
  ),
  &apos;;&apos;,                      /* separator only if both exist */
  &apos;&apos;
) &amp;
IF(
  ISBLANK({!varRespParaEmail}),
  &apos;&apos;,
  {!varRespParaEmail}
)</expression>
    </formulas>
